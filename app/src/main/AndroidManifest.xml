<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.luckygameproj">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".LoGinActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".StartGameActivity" />
        <activity android:name=".startActivity" />
        <activity android:name=".ListScoreActivity"></activity>
        <service
            android:name=".HelloService"
            android:enabled="true" />
        <receiver android:name="SignUpBroadcastReceiver">
             <intent-filter>
                <action android:name="com.example.user.luckygameproj.A_CUSTOM_INTENT">
                </action>
             </intent-filter>
        </receiver>



    </application>

</manifest>